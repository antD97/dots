{
    "sync.autoDownload": false,
    "sync.autoUpload": false,
    "sync.gist": "33568d548038f932b08033f3461b0366",
    "sync.forceUpload": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.fontFamily": "'Source Code Pro', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "selection",
    "java.configuration.checkProjectSettingsExclusions": false,
    "cSpell.userWords": [
        "mutators"
    ],

    /*
        * --- main colors ---
        *
        * gray 1            #161616         editor background
        * gray 2            #222222
        * gray 3            #333333
        * gray 4            #404040
        * gray 5            #505050
        * gray 6            #666666
        *
        * white             #c5c5c5         main text
        * bright white      #efefef         stand out text
        *
        * yellow            #ffe863
        * orange            #fea63c
        * green             #b7ce42
        * blue              #42717b
        * bright blue       #66aabb
        * blue gray         #6d878d
        * dark blue         #2b4a52
        * red               #e84f4f
        * dark ged          #d23d3d
        */

    "workbench.colorCustomizations": {

        // activity bar
        "activityBar.background": "#333333",
        "activityBar.border": "#00000000",
        "activityBar.dropBackground": "#6d878d80",
        "activityBar.foreground": "#efefef",
        "activityBar.inactiveForeground": "#c5c5c5",

        "activityBarBadge.background": "#66aabb",
        "activityBarBadge.foreground": "#efefef",

        // badge
        "badge.background": "#66aabb",
        "badge.foreground": "#efefef",

        // breadcrumb
        "breadcrumb.activeSelectionForeground": "#efefef",
        "breadcrumb.background": "#161616",
        "breadcrumb.focusForeground": "#efefef",
        "breadcrumb.foreground": "#c5c5c5",
        "breadcrumbPicker.background": "#161616",

        // button
        "button.background": "#505050",
        "button.foreground": "#efefef",
        "button.hoverBackground": "#666666",

        // checkbox
        "checkbox.background": "#ff00ff",
        "checkbox.border": "#ff00ff",
        "checkbox.foreground": "#ff00ff",

        // contrast border
        "contrastActiveBorder": "#00000000",
        "contrastBorder": "#00000000",

        // debug
        "debugExceptionWidget.background": "#ff00ff",
        "debugExceptionWidget.border": "#ff00ff",
        "debugToolBar.background": "#ff00ff",
        "debugToolBar.border": "#00000000",

        // description
        "descriptionForeground": "#c5c5c5",

        // diff
        "diffEditor.border": "#ff00ff",
        "diffEditor.insertedTextBackground": "#b7ce4218",
        "diffEditor.insertedTextBorder": "#00000000",
        "diffEditor.removedTextBackground": "#d23d3d18",
        "diffEditor.removedTextBorder": "#00000000",

        // dropdown
        "dropdown.background": "#404040",
        "dropdown.border": "#00000000",
        "dropdown.foreground": "#efefef",
        "dropdown.listBackground": "#404040",

        // --- editor --- //
        "editor.background": "#161616",
        "editor.foreground": "#c5c5c5",

        // find
        "editor.findMatchBackground": "#505050",
        "editor.findMatchBorder": "#00000000",
        "editor.findMatchHighlightBackground": "#50505040",
        "editor.findMatchHighlightBorder": "#00000000",

        "editor.findRangeHighlightBackground": "#333333",
        "editor.findRangeHighlightBorder": "#00000000",

        "editor.rangeHighlightBackground": "#222222",
        "editor.rangeHighlightBorder": "#00000000",

        // highlight
        "editor.hoverHighlightBackground": "#333333",

        "editor.lineHighlightBackground": "#222222",
        "editor.lineHighlightBorder": "#00000000",

        "editor.selectionBackground": "#6d878d60",
        "editor.selectionForeground": "#c5c5c5",
        "editor.inactiveSelectionBackground": "#6d878d30",

        "editor.selectionHighlightBackground": "#50505080",
        "editor.selectionHighlightBorder": "#00000000",

        // snippet tabstop
        "editor.snippetFinalTabstopHighlightBackground": "#50505080",
        "editor.snippetFinalTabstopHighlightBorder": "#00000000",
        "editor.snippetTabstopHighlightBackground": "#50505080",
        "editor.snippetTabstopHighlightBorder": "#00000000",

        // bracket match
        "editorBracketMatch.background": "#50505080",
        "editorBracketMatch.border": "#c5c5c5",

        // code lens
        "editorCodeLens.foreground": "#ff00ff",

        // word highlight
        "editor.wordHighlightBackground": "#50505080",
        "editor.wordHighlightBorder": "#00000000",
        "editor.wordHighlightStrongBackground": "#42717b80",
        "editor.wordHighlightStrongBorder": "#00000000",

        // cursor
        "editorCursor.background": "#161616",
        "editorCursor.foreground": "#efefef",

        // error
        "editorError.border": "#00000000",
        "editorError.foreground": "#e84f4f",

        // editor group
        "editorGroup.border": "#505050",
        "editorGroup.dropBackground": "#6d878d80",
        "editorGroup.emptyBackground": "#161616",
        "editorGroup.focusedEmptyBorder": "#ff0000",

        // unknown
        "editor.focusedStackFrameHighlightBackground": "#ff00ff",

        "editor.stackFrameHighlightBackground": "#ff00ff",

        // --- editor end --- //
